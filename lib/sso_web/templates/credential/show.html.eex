<h2>Show Credential</h2>

<ul>

  <li>
    <strong>Email:</strong>
    <%= @credential.email %>
  </li>

  <li>
    <strong>Password hash:</strong>
    <%= @credential.password_hash %>
  </li>

  <li>
    <strong>Sso:</strong>
    <%= @credential.sso_id %>
  </li>

</ul>

<span><%= link "Edit", to: credential_path(@conn, :edit, @credential) %></span>
<span><%= link "Back", to: credential_path(@conn, :index) %></span>
